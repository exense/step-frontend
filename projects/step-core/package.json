    "@angular-devkit/schematics": "19.2.14",
    "@angular/material": "19.2.18",
    "@angular/router": "19.2.14",
    "@angular/material-luxon-adapter": "19.2.18",
    "@types/luxon": "3.4.2",
    "@angular/cdk": "19.2.18",
    "@angular/cdk-experimental": "19.2.18",
    "@schematics/angular": "20.0.1",
    "@types/uuid": "8.3.4",
    "uuid": "8.3.2",
    "rxjs": "7.4.0",
    "ace-builds": "1.17.0",
    "angular-split": "19.0.0",
    "luxon": "3.4.4",
    "@angular-devkit/build-angular": "19.2.14"

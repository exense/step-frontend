    "@angular-devkit/schematics": "14.2.7",
    "@angular/material": "14.2.6",
    "@angular/material-luxon-adapter": "14.2.6",
    "@types/luxon": "2.3.2",
    "@angular/cdk": "14.2.6",
    "@schematics/angular": "14.2.7",
    "@types/angular": "1.7.4",
    "@types/uuid": "8.3.4",
    "uuid": "8.3.2",
    "rxjs": "7.4.0",
    "angular-split": "14.1.0"
  },
